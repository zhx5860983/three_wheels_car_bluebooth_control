ndef on_forever():\n    global time_current, dt, time_last, velo_l, velo_r\n    if start:\n        time_current = input.running_time()\n        dt = 1000\n        time_last = time_current\n        velo_l = velo_l + inv_acceleration_time * (velo_l_target - velo_l) * dt\n        velo_r = velo_r + inv_acceleration_time * (velo_r_target - velo_r) * dt\n        wuKong.set_all_motor(velo_l, velo_r)\n    else:\n        basic.clear_screen()\nbasic.forever(on_forever)\n","pxt.json":"{\n    \"name\": \"control - Copy\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"microphone\": \"*\",\n        \"bluetooth\": \"*\",\n        \"wuKong\": \"github:elecfreaks/pxt-wukong#v1.5.6\",\n        \"pxt-sonar\": \"github:microsoft/pxt-sonar#v0.0.6\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"main.py\"\n    ],\n    \"preferredEditor\": \"blocksprj\"\n}\n"}},{"timestamp":1727975158017,"editorVersion":"7.0.42","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\"yAJ)Qfp7]~@JtHlFdDvg\">straight_pressed</variable><variable id=\"/Ya{5~TKA)`bw$MXo%73\">velo_multiplier_r</variable><variable id=\"-PX*zH-s6t/lM!]vQt7p\">full_speed</variable><variable id=\"E77fI9bIBFL{c74]oq3$\">speed_limit</variable><variable id=\"~l)gf;)~b~E?8)nn(2.J\">velo_multiplier_l</variable><variable id=\"3E_^gdUZ$:R*L/fJ1tQq\">start</variable><variable id=\"qrcPl)gARZ~]hPd(*U=|\">velo_l_target</variable><variable id=\"o,CiC4Q9.j@~t/Yx~P%6\">velo_r_target</variable><variable id=\"Xe6|7[M.0YjCJD#w4Nf,\">velo_l</variable><variable id=\"Z#LANwu;-2HtP1/($vSJ\">velo_r</variable><variable id=\"7m|#|,VrtDUT[P2h04#r\">time_current</variable><variable id=\"@HZ+6$Qx@L$lF2%vUb1A\">dt</variable><variable id=\"}E%[(jQzwt0dnDvL0{zh\">time_last</variable><variable id=\"PW|9j@upt?qGM^:S{B]S\">inv_acceleration_time</variable><variable id=\"?3$#b15W3{*-/5AU!d)i\">acceleration_time<